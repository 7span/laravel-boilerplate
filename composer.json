{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "aws/aws-sdk-php": "^3.341",
        "darkaonline/l5-swagger": "^9.0",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "laravel-notification-channels/onesignal": "^2.8",
        "laravel/framework": "^12.7.2",
        "laravel/horizon": "^5.31",
        "laravel/pulse": "^1.4",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.5",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "opcodesio/log-viewer": "^3.15",
        "plank/laravel-mediable": "^6.3",
        "predis/predis": "^2.3",
        "spatie/laravel-littlegatekeeper": "^1.1",
        "spatie/laravel-permission": "^6.16",
        "spatie/laravel-query-builder": "^6.3"
    },
    "require-dev": {
        "beyondcode/laravel-query-detector": "^2.1",
        "ekino/phpstan-banned-code": "^3.0",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/boost": "^1.0",
        "laravel/pint": "^1.22",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.4",
        "tightenco/duster": "^2.7"
    },
    "autoload": {
        "files": [
            "app/Libraries/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "App\\Console\\Commands\\": "app/Console/Commands/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
